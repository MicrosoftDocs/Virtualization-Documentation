{
  "build": {
    "content": [
      {
        "files": ["**/**.md", "**/**.yml"],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],

    "globalMetadata": {
      "breadcrumb_path": "/virtualization/toc.json",
      "extendBreadcrumb": true,
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/Virtualization-Documentation",
      "feedback_product_url": "https://github.com/MicrosoftDocs/Virtualization-Documentation/issues"
    },
    "fileMetadata": {
      "ms.prod": {
         "**/*.md": "windows-server"
      },
      "ms.technology": {
         "**/*.md": "windows-containers"
      }
    },
    "template": [],
    "dest": "virtualization/windowscontainers"
  }
}
